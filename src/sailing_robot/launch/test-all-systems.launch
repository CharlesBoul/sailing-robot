<launch>
    <!--
    General test file to launch all existing nodes with dummy values
    -->

    <!--
    Parameters
    -->

    <rosparam file="$(find sailing_robot)/launch/parameters/default.yaml" command="load"/>

    <!--
    Nodes
    -->
    <node name="publish_dummy_apparent_wind" pkg='sailing_robot' type="publish_apparent_wind_direction"/>
    <node name="publish_dummy_heading" pkg='sailing_robot' type="publish_heading"/>
    <node name="publish_dummy_position" pkg='sailing_robot' type="publish_position"/>
    <node name = "heading_control" pkg="sailing_robot" type = "heading_control" />
    <node name = "tack" pkg="sailing_robot" type = "tack" />
    <node name = "pid_rudder" pkg="sailing_robot" type = "pid_rudder" />

    <!--
    Launch Bridge to Arduino
    -->
  <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
    <param name="port" value="/dev/ttyACM0"/>
    <param name="baud" value="9600"/>
  </node>

</launch>
